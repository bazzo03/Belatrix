<apps>
	<app name="jira">
		<operation name="createIssue" type="POST" path="issue/">
				<![CDATA[{"fields":{"project":{"key": "%S"},"summary": "%s","description": "%S","issuetype": {"name": "%s"}}}]]>
		</operation>
		<operation name="addCommentIssue" type="POST" path="issue/%s/comment">
				<![CDATA[{"body":"%s"}]]>
		</operation>
				<operation name="attachmentIssue" type="POST" path="issue/%s/attachments">
				<![CDATA[{}]]>
		</operation>
	</app>
	<app name="salesforce">
		<operation name="EditLead" type="POST" path="issue/">
				<![CDATA[
				{ "name":" %s ", "type": %S , "date":null }
				]]>
		</operation>
	</app>
	<app name="trello">

		<operation name="createCard" type="POST" path="cards">
				<![CDATA[{"name":"%s","idList":"5942a9e4465cc1972dfb25c6","desc":"%s","pos": "top"}]]>
		</operation>
		<operation name="editCard" type="PUT" path="cards/%s">
				<![CDATA[{"name":"%s","idList":"5942a9e4465cc1972dfb25c6","desc":"%s","pos": "top"}]]>
		</operation>
		<operation name="addCommentCard" type="POST" path="cards/%s/actions/comments">
				<![CDATA[{"text":"%s"}]]>
		</operation>
				<operation name="attachmentCard" type="POST" path="cards/%s/attachments">
				<![CDATA[{"url":"%s","idList":"5942a9e4465cc1972dfb25c6","file": "%s","name":"%s"}]]>
		</operation>

	</app>
</apps>